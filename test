# -*- coding: utf-8 -*-

import os
import csv

with open('./exemple_csv.csv') as permis:
  permisReader = csv.reader(permis)
  permisData = list(permisReader)

newFile = list()

for i in range(0, len(permisData)):
  liste = str(permisData[i]).split(';')

  parcelles = liste[12].split('|')
  ligne = liste
  
  for j in range(0, len(parcelles)):
       if parcelles[j].startswith(' '):
           parcelles[j] = parcelles[j][1:]
       ligne[12] = parcelles[j]
       ligneConcat = ligne[0]
       for k in range(1, len(ligne)):
           ligneConcat = ligneConcat + ';' + ligne[k]
       newFile.append(ligneConcat)

with open('output.csv','wb') as output:
  wr = csv.writer(output, quoting=csv.QUOTE_NONE, escapechar='\\')
  for i in range(0, len(newFile)):
      rowToWrite = [newFile[i]]
      print rowToWrite
      wr.writerow(rowToWrite)
